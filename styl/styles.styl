*
	margin 0
	padding 0
	outline none
a
	color: #fefefe
	&:hover
		text-decoration: none
	&:focus
		text-decoration: none
		outline: 0
		
.video_wrapper
	margin 0 auto
	position relative
	&:before
		content ''
		height 0
		padding-top 56.25%
		display block
	img,
	iframe
		position absolute
		width 100%
		height 100%
		display block
		background-color #fff
		border-radius 3px
		border: 0
		top 0
		left 0
		right 0
		bottom 0
		cursor pointer

.button
	display block
	border: 1px solid  rgba(7, 14, 23, .45);
	border-radius: 10px;
	background-image: linear-gradient( 0deg, rgb(73,96,175) 0%, rgb(72,198,239) 100%);
	box-shadow: 0px 5px 11px 0px rgba(0, 0, 0, 0.15), 0px 5px 11px rgba(#ffffbe, 0.38) inset;
	width: 260px;
	height: 57px;
	font-size: 18px;
	font-family: "HelveticaNeueCyrBold", sans-serif;
	color: rgb(255, 255, 255);
	text-transform: uppercase;
	line-height: 57px
	text-align: center
	margin: 70px auto 0
	&:hover
		border 1px solid rgba(4, 8, 13, .45);
		border-radius: 10px;
		background-image: linear-gradient( 0deg, rgb(72,198,239) 0%, rgb(73,96,175) 100%);
		box-shadow: 0px 5px 11px 0px rgba(0, 0, 0, 0.15),inset 0px 3px 7px 0px rgba(0, 0, 0, 0.42);
		color #ffffff
	&:focus
		color #ffffff

h3
	font-size: 30px;
	font-family: "HelveticaNeueCyrBlack", sans-serif;
	color: #ffffff
	text-transform: uppercase;
	line-height: 1.1;
	text-align center
	&:after
		content ''
		display block
		background-color: rgb(#fff);
		width: 83px;
		height: 4px;
		margin: 20px auto 0



